def generate_sitemap(pages):
    """
    Auto-generate XML sitemap
    """
    sitemap_entries = []
    
    for page in pages:
        entry = {
            'url': page['url'],
            'lastmod': page['updated_at'],
            'priority': page['priority'],
            'changefreq': 'weekly'
        }
        sitemap_entries.append(entry)
    
    # Convert to XML format and submit to search engines
